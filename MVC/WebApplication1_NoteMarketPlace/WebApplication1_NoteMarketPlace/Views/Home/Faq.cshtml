                    <li class="nav-item">
                        @Html.ActionLink("Contact Us", "Contact", "Home", null, new { @class = "nav-link" })
                    </li>
                    <li class="nav-item">
                        @if (Session["EmailID"] == null)
                        {
                            @Html.ActionLink("Login", "Login", "Account", null, new { @class = "btn btn-primary btn-lg btn-block btn_login" })
                        }
                        else
                        {
                            @Html.ActionLink("Logout", "Logout", "Account", null, new { @class = "btn btn-primary btn-lg btn-block btn_login" })
                        }
                    </li>

